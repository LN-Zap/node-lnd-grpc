{
  "name": "lnd-grpc",
  "version": "0.4.0-beta.5",
  "description": "Repository of lnd rpc protocol files and utilities for working with them",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "test": "DEBUG=lnrpc* babel-tape-runner test/*.js | tap-spec",
    "lint": "prettier-eslint --write \"$PWD/**/*.{js,json,md}\"",
    "build": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist",
    "clean": "rimraf -r ./dist",
    "preversion": "npm test",
    "prepack": "npm run build"
  },
  "files": [
    ".babelrc",
    "dist",
    "src",
    "proto"
  ],
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LN-Zap/node-lnd-grpc.git"
  },
  "keywords": [
    "bitcoin",
    "lightning-network",
    "lnd",
    "grpc"
  ],
  "author": "Tom Kirkpatrick <tkp@kirkdesigns.co.uk> (http://www.kirkdesigns.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LN-Zap/node-lnd-grpc/issues"
  },
  "homepage": "https://github.com/LN-Zap/node-lnd-grpc",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-source-map-support": "2.1.1",
    "babel-tape-runner": "3.0.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "lnd-binary": "0.3.15",
    "prettier": "1.18.2",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "source-map-support": "0.5.16",
    "split2": "3.1.1",
    "tap-spec": "5.0.0",
    "tape": "^4.13.2",
    "tape-promise": "4.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^0.8.1",
    "@ln-zap/proto-loader": "0.5.4",
    "debug": "4.1.1",
    "javascript-state-machine": "3.1.0",
    "lndconnect": "0.2.10",
    "lodash.defaultsdeep": "4.6.1",
    "semver": "^7.1.3",
    "untildify": "4.0.0",
    "validator": "^13.0.0"
  }
}
