{
  "name": "lnd-grpc",
  "version": "0.4.8",
  "description": "Repository of lnd rpc protocol files and utilities for working with them",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "test": "DEBUG=lnrpc* babel-tape-runner test/*.js | tap-spec",
    "test-tor": "DEBUG=lnrpc* TOR=true babel-tape-runner test/grpc.test.js | tap-spec",
    "lint": "prettier-eslint --write \"$PWD/**/*.{js,json,md}\"",
    "build": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist",
    "clean": "rimraf -r ./dist",
    "preversion": "npm test",
    "prepack": "npm run build"
  },
  "files": [
    ".babelrc",
    "dist",
    "src",
    "proto"
  ],
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LN-Zap/node-lnd-grpc.git"
  },
  "keywords": [
    "bitcoin",
    "lightning-network",
    "lnd",
    "grpc"
  ],
  "author": "Tom Kirkpatrick <tkp@kirkdesigns.co.uk> (http://www.kirkdesigns.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LN-Zap/node-lnd-grpc/issues"
  },
  "homepage": "https://github.com/LN-Zap/node-lnd-grpc",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-tape-runner": "3.0.0",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.12.0",
    "lnd-binary": "^0.3.22",
    "prettier": "^2.3.2",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "source-map-support": "0.5.19",
    "split2": "^3.2.2",
    "tap-spec": "5.0.0",
    "tape": "^5.3.1",
    "tape-promise": "4.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.3.6",
    "@grpc/proto-loader": "0.6.4",
    "debug": "4.2.0",
    "get-port": "5.1.1",
    "javascript-state-machine": "3.1.0",
    "lndconnect": "0.2.10",
    "lodash.defaultsdeep": "4.6.1",
    "semver": "7.1.3",
    "untildify": "4.0.0",
    "validator": "13.1.17"
  }
}
